<%= form_for(@meeting) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>

    <%= f.label :initiative %>
    <%= f.hidden_field(:initiative_id, :value => 1) %>

    <%= f.label :date %>
    <%= f.date_field :datetime, class: 'form-control' %>

    <%= f.submit 'Submit', class: 'btn btn-primary' %>
<% end %>
