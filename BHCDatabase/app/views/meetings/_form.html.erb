<%= form_for(@meeting) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>

    <%= f.hidden_field(:initiative_id, :value => Initiative.find(params[:initiative_id]).id) %>

    <%= f.label :datetime %>
    <%= f.datetime_select :datetime, :end_year => Date.today.year %>

    <%= f.submit 'Submit', class: 'btn btn-primary' %>
<% end %>
